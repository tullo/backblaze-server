{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "matchPackageNames": ["github.com/aws/aws-sdk-go", "github.com/dgraph-io/badger/v2"],
      "automerge": true
    }
  ],
  "automerge": true,
  "commitBodyTable": true  
}
